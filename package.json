{
  "name": "emaily_server",
  "version": "1.0.0",
  "description": "Server related to collecting feedback about apps.",
  "main": "server/index.js",
  "scripts": {
    "server": "node server/index.js",
    "server_dev": "nodemon",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server_dev\" \"npm run client\"",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bondarukoleh/react_web_app.git"
  },
  "keywords": [],
  "author": "Oleh Bondaruk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bondarukoleh/react_web_app/issues"
  },
  "homepage": "https://github.com/bondarukoleh/react_web_app#readme",
  "dependencies": {
    "concurrently": "5.1.0",
    "cookie-session": "1.4.0",
    "express": "4.17.1",
    "mongoose": "5.9.3",
    "passport": "0.4.1",
    "passport-google-oauth20": "2.0.0"
  },
  "engines": {
    "node": "12.13.1"
  },
  "devDependencies": {
    "nodemon": "2.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "exec": "node ./server/index.js"
  }
}
